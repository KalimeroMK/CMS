{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "laravel/framework": "^7.0",
    "guzzlehttp/guzzle": "^6.3",
    "sabre/xml": "^2.1",
    "cohensive/embed": "^5.5",
    "intervention/image": "^2.5",
    "barryvdh/laravel-debugbar": "~3.2.0",
    "filp/whoops": "^2.5",
    "phpspec/phpspec": "^6.0",
    "spatie/laravel-responsecache": "^6.6",
    "kalimeromk/update-htmldom": "dev-master",
    "kalimeromk/update-thujohn-rss": "dev-master",
    "spatie/laravel-analytics": "^3.7",
    "spatie/laravel-permission": "^3.13",
    "laravel/helpers": "^1.1",
    "laravelcollective/html": "^6.0",
    "ext-curl": "*",
    "ext-json": "*",
    "laravel/ui": "^2.0",
    "ext-mysqli": "*",
    "kalnoy/nestedset": "^5.0",
    "ext-libxml": "*",
    "fruitcake/laravel-cors": "^2.0",
    "orangehill/iseed": "^2.6",
    "laravelium/sitemap": "^7.0",
    "spatie/laravel-feed": "^2.6"
  },
  "require-dev": {
    "symfony/dom-crawler": "~3.1",
    "symfony/css-selector": "~3.1",
    "fzaninotto/faker": "~1.9.1",
    "laravel/tinker": "^2.0",
    "mockery/mockery": "1.2.4",
    "phpunit/phpunit": "^8.5",
    "fideloper/proxy": "^4.0",
    "barryvdh/laravel-ide-helper": "^2.6"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
